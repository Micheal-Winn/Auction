generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  name     String
  password String
}

model Post {
  id    String @id @default(uuid())
  title String
  image String
  price Int
  
}
